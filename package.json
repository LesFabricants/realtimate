{
  "name": "realtimate",
  "version": "0.0.0",
  "description": "An Atlas Realm Build Environment",
  "scripts": {
    "build": "npx tsc && npm run assets && npm run types",
    "assets": "copyfiles -a -u 1 src/assets/**/*.template dist",
    "types": "copyfiles -f ./src/types.d.ts dist",
    "watch": "npx tsc-watch --onSuccess 'npm run assets && npm run types' --onCompilationComplete 'npm install -g'",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint -c .eslintrc.json src/"
  },
  "main": "dist/empty.js",
  "files": [
    "dist/",
    "misc/"
  ],
  "types": "dist/types.d.ts",
  "bin": {
    "realtimate": "./dist/index.js"
  },
  "repository": "github:LesFabricants/realtimate",
  "author": "Les Fabricants",
  "license": "GPL-3.0-or-later",
  "dependencies": {
    "@types/body-parser": "^1.19.2",
    "@types/chalk": "^2.2.0",
    "@types/express": "^4.17.17",
    "@types/figlet": "^1.5.6",
    "@types/readline-sync": "^1.4.4",
    "@vercel/ncc": "^0.36.1",
    "atlas-app-services-cli": "^1.0.3",
    "body-parser": "^1.20.2",
    "chalk": "^4.1.1",
    "commander": "^11.0.0",
    "dotenv": "^16.3.1",
    "ejson": "^2.2.3",
    "express": "^4.18.2",
    "figlet": "^1.6.0",
    "mime-types": "^2.1.35",
    "mongodb": "^5.7.0",
    "mongodb-memory-server": "^8.14.0",
    "readline": "^1.3.0",
    "readline-sync": "^1.4.10",
    "typescript-source-graph": "^1.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.4.9",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "copyfiles": "^2.4.1",
    "eslint": "^8.55.0",
    "eslint-config-standard-with-typescript": "^37.0.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-n": "^16.4.0",
    "eslint-plugin-promise": "^6.1.1",
    "tsc-watch": "^6.0.4",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=14 <=20"
  },
  "release": {
    "branches": [
      "main",
      {
        "name": "develop",
        "prerelease": true
      }
    ]
  }
}